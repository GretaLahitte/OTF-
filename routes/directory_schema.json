{
    "Account": {
        "collection": "accounts",
        "schema": {
            "login": "String",
            "password": "String",
            "profile": []
        }
    },
    "Logs": {
        "collection": "logs",
        "schema": {
            "date": "String",
            "event": "String",
            "message": "String",
            "ip": "String",
            "session": "String",
            "user_id": {
                "type": "ObjectId",
                "ref": "Account"
            }
        }
    }

}