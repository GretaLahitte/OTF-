{{> menu}}
<div class="container-fluid">
    <div class="row-fluid">
        <div class="col-xs-12 col-sm-12 col-md-12">
            <h3>List Users</h3>
        </div>
    </div>
    <div class="row-fluid">
        {{#each result}}
            <div class="row-fluid">
                <div class="col-xs-12 col-sm-8 col-md-4 col-lg-4">
                    <p>Id : {{_id}}</p>

                    <p>Login : {{login}}</p>

                    <p>Password : {{password}}</p>

                    <p>Version : {{__v}}</p>

                    <p></p>
                </div>
            </div>
        {{/each}}
    </div>
</div>